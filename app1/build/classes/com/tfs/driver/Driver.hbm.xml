<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.tfs.driver.model.impl.DriverImpl" table="tfs_driver">
	 <id name="id" column="id" >
	 	<generator class="increment"/>
	 </id>
	 <property name="name" column="name" />
	 <property name="operatorId" column="operator_id" />
	 <property name="vehicleDescription" column="vehicle_description" />
	 <property name="available" column="available" />
 </class>
</hibernate-mapping>
